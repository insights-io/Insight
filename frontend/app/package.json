{
  "name": "@insight/app",
  "private": true,
  "version": "0.0.0-development",
  "description": "Insight app",
  "license": "MIT",
  "author": "Insight Engineering",
  "homepage": "https://github.com/insights-io/insight#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insights-io/insight.git"
  },
  "bugs": {
    "url": "https://github.com/insights-io/insight/issues"
  },
  "scripts": {
    "dev": "next",
    "dev:proxy": "PROXY=true yarn dev",
    "dev:proxy:staging": "NEXT_PUBLIC_AUTH_API_BASE_URL=https://auth-api.dev.snuderls.eu AUTH_API_BASE_URL=https://auth-api.dev.snuderls.eu NEXT_PUBLIC_SESSION_API_BASE_URL=https://session-api.dev.snuderls.eu SESSION_API_BASE_URL=https://session-api.dev.snuderls.eu yarn dev:proxy",
    "build": "next build",
    "build:deps": "yarn build:deps:l1 && yarn build:deps:l2 && yarn build:deps:l3",
    "build:deps:l1": "yarn workspace @insight/types build",
    "build:deps:l2": "concurrently \"yarn workspace @insight/service-proxy build\" \"yarn workspace @insight/storybook build\" \"yarn workspace @insight/testing build\" \"yarn workspace @insight/sdk build\" \"yarn workspace @insight/elements build\"",
    "build:deps:l3": "concurrently \"yarn workspace @insight/next-testing build\" \"yarn workspace @insight/next-storybook build\"",
    "start": "next start",
    "test": "jest --env=jest-environment-jsdom-sixteen",
    "test:e2e": "testcafe chrome e2e/specs",
    "lint": "concurrently \"npm run prettier\" \"npm run eslint\"",
    "eslint": "eslint --max-warnings 0 --ext .ts 'src/**/*' 'e2e/**/*'",
    "prettier": "prettier -l 'src/**/*' 'e2e/**/*'",
    "prettier:fix": "prettier -l 'src/**/*' 'e2e/**/*' --write",
    "storybook": "start-storybook -s ./public",
    "storybook:build": "build-storybook",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "pretest": "yarn prebuild",
    "prebuild": "yarn build:deps"
  },
  "dependencies": {
    "@amcharts/amcharts4": "4.10.4",
    "@amcharts/amcharts4-geodata": "4.1.17",
    "@stripe/stripe-js": "1.9.0",
    "@stripe/react-stripe-js": "1.1.2",
    "@insight/service-proxy": "0.0.0-development",
    "@insight/sdk": "0.0.0-development",
    "@insight/elements": "0.0.0-development",
    "baseui": "9.101.0",
    "date-fns": "2.16.1",
    "jaeger-client": "3.18.1",
    "next": "9.5.3",
    "next-cookies": "2.0.3",
    "next-pwa": "3.1.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "6.8.6",
    "react-icons": "3.11.0",
    "react-window": "1.8.5",
    "react-window-infinite-loader": "1.0.5",
    "react-virtualized-auto-sizer": "1.0.2",
    "styletron-engine-atomic": "1.4.6",
    "styletron-react": "5.2.7",
    "swr": "0.3.5"
  },
  "devDependencies": {
    "@insight/storybook": "0.0.0-development",
    "@insight/next-storybook": "0.0.0-development",
    "@insight/testing": "0.0.0-development",
    "@insight/next-testing": "0.0.0-development",
    "@insight/types": "0.0.0-development",
    "@next/bundle-analyzer": "9.5.3",
    "@types/jaeger-client": "3.15.4",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/uuid": "8.3.0",
    "@types/styletron-engine-atomic": "1.1.0",
    "@types/styletron-react": "5.0.2",
    "@types/react-window-infinite-loader": "1.0.3",
    "@types/react-virtualized-auto-sizer": "1.0.0",
    "uuid": "8.3.0",
    "jsqr": "1.3.1",
    "speakeasy": "2.0.0"
  }
}

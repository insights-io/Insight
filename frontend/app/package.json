    "@visx/legend": "1.3.0",
    "@visx/grid": "1.3.0",
    "@visx/responsive": "1.3.0",
    "@stripe/stripe-js": "1.11.0",
    "@stripe/react-stripe-js": "1.1.2",
    "@rebrowse/service-proxy": "0.0.0-development",
    "@rebrowse/sdk": "0.0.0-development",
    "@rebrowse/elements": "0.0.0-development",
    "baseui": "9.106.3",
    "date-fns": "2.16.1",

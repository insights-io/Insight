{
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "builds": [
    { "src": "frontend/app/package.json", "use": "@now/next" },
    { "src": "frontend/try/package.json", "use": "@now/next" }
  ],
  "rewrites": [
    { "source": "/try/:match*", "destination": "/frontend/try/:match*" },
    { "source": "/(.*)", "destination": "/frontend/app/$1" }
  ]
}
